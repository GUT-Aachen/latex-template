	keywords = {Deformation: Experimental Studies, Igneous Rocks: Trachyte, Mantle: Experimental Studies, Metamorphic Rocks: Marble, Sedimentary Rocks: Properties},
	file = {Mogi - 1971 - Fracture and flow of rocks under high triaxial com.pdf:D\:\\Benutzer\\Biebricher\\Dokumente\\40 Literatur\\MERID\\Zotero\\storage\\LL6NWNZA\\Mogi - 1971 - Fracture and flow of rocks under high triaxial com.pdf:application/pdf;Snapshot:D\:\\Benutzer\\Biebricher\\Dokumente\\40 Literatur\\MERID\\Zotero\\storage\\VK4324EY\\JB076i005p01255.html:text/html}
