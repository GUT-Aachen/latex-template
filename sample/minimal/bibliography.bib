	note = {{ZSCC}: 0000008},
	keywords = {Drilling large cores, Fractured rock, Sample recovery, Large Probes},
	file = {McDermott et al. - 2003 - Recovery of undisturbed highly fractured bench sca.pdf:D\:\\Benutzer\\Biebricher\\Dokumente\\40 Literatur\\MERID\\Zotero\\storage\\VLIZQXCD\\McDermott et al. - 2003 - Recovery of undisturbed highly fractured bench sca.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Benutzer\\Biebricher\\Dokumente\\40 Literatur\\MERID\\Zotero\\storage\\RFXEDW2C\\S0013795202002806.html:text/html}
