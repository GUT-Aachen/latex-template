	urldate = {2020-04-24},
	date = {2001-01-01},
	langid = {english},
	note = {{ZSCC}: 0000219},
	file = {klein et al_2001_mechanical behaviour and failure mode (Paper).pdf:D\:\\Benutzer\\Biebricher\\Dokumente\\40 Literatur\\MERID\\Zotero\\storage\\DMU7AXSE\\klein et al_2001_mechanical behaviour and failure mode (Paper).pdf:application/pdf;ScienceDirect Snapshot:D\:\\Benutzer\\Biebricher\\Dokumente\\40 Literatur\\MERID\\Zotero\\storage\\ATAS6ZER\\S1464189501000175.html:text/html}
